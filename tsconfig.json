{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": "./",
    "paths": {
      "@/src/*": ["/*"],
      "views/*": ["views/*"],
      "components/*": ["views/components/*"]
    },
    "moduleResolution": "node10",
    "module": "ESNext",
    "target": "ESNext",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  }
}
